{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "Gatsby with Netlify and microCMS",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "       run-p lint:*",
    "lint:script": "eslint -c ./.eslintrc.js './src/**/*.{ts,tsx}'",
    "lint:style": " stylelint --config ./.stylelintrc.js './src/**/*.{css,scss}'",
    "lint:spell": " yarn cspell './src/**/*.{ts,tsx,css,scss}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "cspell",
      "prettier --write",
      "eslint -c .eslintrc.js --fix"
    ],
    "src/**/*.{css,scss}": [
      "cspell",
      "prettier --write",
      "stylelint --config .stylelintrc.js --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": [
    ">= 1% in jp",
    "not ie 11"
  ],
  "dependencies": {
    "gatsby": "^4.2.0",
    "gatsby-plugin-gatsby-cloud": "^4.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@cspell/dict-css": "^1.0.12",
    "@cspell/dict-html": "^1.1.9",
    "@cspell/dict-node": "^1.0.12",
    "@cspell/dict-npm": "^1.0.16",
    "@cspell/dict-typescript": "^1.0.19",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "cspell": "^5.13.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-sass": "^5.2.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.3",
    "gatsby-plugin-typegen": "^2.2.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^8.0.0",
    "postcss-color-hex-alpha": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-image-set-function": "^4.0.2",
    "postcss-initial": "^3.0.4",
    "postcss-media-minmax": "^5.0.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sort-package-json": "^1.53.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.5.2"
  }
}
